{
  "name": "fadlisaad/php-touch-n-go",
  "description": "Basic implementation of Touch N Go transaction using PHP",
  "type": "library",
  "require": {
    "php": ">=7.4",
    "illuminate/support": "5.6.* || 5.7.* || 5.8.* || 6.0.*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Fadli Saad",
      "email": "fadlisaad@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "PHPTouchNGo\\": "src/PHPTouchNGo/"
    },
    "files": [
      "src/PHPTouchNGo/TouchNGo.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "PHPTouchNGo\\PHPTouchNGoServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^3.8"
  }
}
